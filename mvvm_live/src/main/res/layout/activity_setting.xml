<?xml version="1.0" encoding="utf-8"?>
<layout xmlns:bind="http://schemas.android.com/apk/res-auto">

    <data>

        <variable
            name="aboutUsClick"
            type="com.ygsoft.utilslib.utils.OnClickHandler" />
    </data>

    <LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
        xmlns:app="http://schemas.android.com/apk/res-auto"
        xmlns:tools="http://schemas.android.com/tools"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:orientation="vertical"
        tools:context="com.mp4.androidmodel.view.activity.SettingActivity">

        <include
            layout="@layout/layout_click_item"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            bind:content="@{@string/welcome_use}"
            bind:clickHandler="@{aboutUsClick}"
            bind:title="@{@string/about_us}" />

    </LinearLayout>
</layout>